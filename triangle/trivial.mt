! This is comment .
let
    const m ~ 7;
   var n: Integer;
   
   proc writeln(var l: array of 20 Char) ~ 
            let var i: Integer 
            in begin i:=0; while (ord(l[i])\= 0) do begin put(l[i]);i:=i+1 end end
        
   
   in
     begin
       writeln(var 'Hello world!');
       n := 2 * m * m;
       putint(n)
      end
